<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/library</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">123456</property>
    <mapping class="org.stip.library.Model.StipBookInfo"/>
    <mapping class="org.stip.library.Model.StipBook"/>
    <mapping class="org.stip.library.Model.StipLibrary"/>
    <mapping class="org.stip.library.Model.StipBorrwoList"/>
    <mapping class="org.stip.library.Model.StipListBookMap"/>
    <mapping class="org.stip.library.Model.StipPersonInfo"/>
    <mapping class="org.stip.library.Model.StipSchool"/>
    
  </session-factory>
</hibernate-configuration>
